{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build NgIcons.Demo",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/src/NgIcons.Demo/NgIcons.Demo.csproj",
        "-c",
        "Debug"
      ],
      "problemMatcher": "$msCompile",
      "group": "build"
    },
    {
      "label": "watch NgIcons.Demo",
      "command": "dotnet",
      "type": "process",
      "args": [
        "watch",
        "run",
        "--project",
        "${workspaceFolder}/src/NgIcons.Demo/NgIcons.Demo.csproj"
      ],
      "problemMatcher": "$msCompile",
      "isBackground": true,
      "group": "build"
    }
  ]
}
